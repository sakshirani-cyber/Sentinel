      "main": "dist-electron/main.js",
      "scripts": {
            "dev": "vite",
            "postinstall": "electron-rebuild -f -w better-sqlite3",
            "build": "vite build",
            "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && tsc -p electron/tsconfig.json && electron .\"",
            "build:electron": "tsc -p electron/tsconfig.json && vite build && electron-builder",
            "build:win": "npm run build:electron -- --win"
      },
      "build": {
            "appId": "com.arrise.sentinel",
            "productName": "Sentinel",
            "directories": {
                  "output": "dist-build"
            },
            "files": [
                  "dist/**/*",
                  "dist-electron/**/*",
                  "package.json"
            ],
            "win": {
                  "target": "nsis"
            },
            "publish": {
                  "provider": "github",
                  "owner": "YOUR_GITHUB_USERNAME",
                  "repo": "YOUR_REPO_NAME"
            }
      }
